{
  "grammars": [
    {
      "name": "mdx",
      "camelcase": "Mdx",
      "scope": "source.mdx",
      "path": ".",
      "file-types": [
        "mdx"
      ],
      "highlights": [
        "queries/highlights-jsx.scm",
        "queries/highlights.scm"
      ],
      "injection-regex": "^(mdx)$"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "MDX grammar for node-tree-sitter",
    "authors": [
      {
        "name": "Javier López Úbeda"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-mdx"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
